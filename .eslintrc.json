{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": [
      "warning",
      2
    ],
    "linebreak-style": [
      "warning",
      "windows"
    ],
    "quotes": [
      "warning",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "eqeqeq":"off"
  }
}
