{
	"recurseDepth": 10,
	"source": {
		"include": ["src", "README.md"]
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"includeDate": false
		}
	},
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "./docs",
		"recurse": true
	}
}
